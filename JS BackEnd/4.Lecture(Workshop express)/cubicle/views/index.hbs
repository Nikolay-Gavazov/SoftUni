      <h1>Browser</h1>
      <form action="/" method="GET">
        <input type="text" class="search" name="search" placeholder="Search...">
        <input type="number" name="from" class="difficulty" placeholder="Difficulty from...">
        <span>-</span>
        <input type="number" name="to" class="difficulty" placeholder="Difficulty to...">
        <input type="submit" value="search">
      </form>
      <div class="cubes">
        {{#each cubes}}
        {{> cubeInfo}}
        {{else}}
        <p class="nothing">No cubes in Database! <a class="sell" href="/create">Create new cube!</a></p>
        {{/each}}
      </div>
