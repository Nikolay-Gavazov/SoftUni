<h1>Browser</h1>
<form action="" method="">
  <input type="text" class="search" name="search" placeholder="Search...">
  <input type="number" name="from" class="price" placeholder="Price from...">
  <span>-</span>
  <input type="number" name="to" class="price" placeholder="Price to...">
  <input type="submit" value="search">
</form>

<div>
  {{#each cars}}
  {{> carInfo}}
  {{else}}
  <p>No cars for sale.<a href="/create">Sell your car!</a></p>
  {{/each}}
</div>